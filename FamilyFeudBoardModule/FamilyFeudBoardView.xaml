<UserControl x:Class="FamilyFeudBoardModule.FamilyFeudBoardView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FamilyFeudBoardModule"             
             xmlns:interfaces="clr-namespace:GameInterfaces;assembly=GameInterfaces"             
             xmlns:prism="http://prismlibrary.com/"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">

        
        <Viewbox Grid.Column="0" VerticalAlignment="Top">
            <ContentControl prism:RegionManager.RegionName="{x:Static interfaces:RegionNames.GameWindow}" />
        </Viewbox>
        
    
</UserControl>
